x>\n          <FilterInnerTitle>도미토리</FilterInnerTitle>\n          {dormitories.map((dormitory) => (\n            <Filters>\n              <LeftSideCheckbox\n                value=\"dormitory\"\n                id={dormitory.id}\n                onClick={handleFilter}\n              />\n              {dormitory.type}\n            </Filters>\n          ))}\n        </FilterInnerBox>\n        <FilterInnerBox>\n          <FilterInnerTitle>일반실</FilterInnerTitle>\n          {general_rooms.map((room) => (\n            <Filters>\n              <LeftSideCheckbox\n                value=\"room\"\n                id={room.id}\n                onClick={handleFilter}\n              />\n              {room.type}\n            </Filters>\n          ))}\n        </FilterInnerBox>\n        <FilterInnerBox>\n          <FilterInnerTitle>무료 서비스</FilterInnerTitle>\n          {free_services.map((service) => (\n            <Filters>\n              <LeftSideCheckbox\n                value=\"service\"\n                id={service.id}\n                onClick={handleFilter}\n              />\n              {service.type}\n            </Filters>\n          ))}\n        </FilterInnerBox>\n        <FilterInnerBox>\n          <FilterInnerTitle>공용 시설</FilterInnerTitle>\n          {shared_facilities.map((facility) => (\n            <Filters>\n              <LeftSideCheckbox\n                value=\"facility\"\n                id={facility.id}\n                onClick={handleFilter}\n              />\n              {facility.type}\n            </Filters>\n          ))}\n        </FilterInnerBox>\n        <FilterInnerBox>\n          <FilterInnerTitle>객실 내 편의 시설</FilterInnerTitle>\n          {room_amenities.map((amenity) => (\n            <Filters>\n              <LeftSideCheckbox\n                value=\"amenity\"\n                id={amenity.id}\n                onClick={handleFilter}\n              />\n              {amenity.type}\n            </Filters>\n          ))}\n        </FilterInnerBox>\n 